        "plt.xticks(rotation ='vertical')\n",
        "plt.show()\n",
        "plt.figure(figsize = (3, 2))\n",
        "plt.figure(figsize = (3,2))\n",
        "sns.heatmap(df[['Estimated Unemployment Rate (%)',\n",
        "                'Estimated Employed',\n",
        "                'Estimated Labour Participation Rate (%)']].corr(),annot=True)\n",
        "plt.show()\n",
        "unemploment = df[[ \"Region\", \"Estimated Unemployment Rate (%)\"]]\n",
